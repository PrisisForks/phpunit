{
    "name": "testomat/phpunit-printer",
    "type": "phpunit-printer",
    "description": ":crystal_ball: PHPUnit printer, assertion and more...",
    "keywords": [
        "narrowspark",
        "testomat",
        "phpunit",
        "printer",
        "error",
        "exception",
        "code-coverage"
    ],
    "homepage": "http://github.com/testomat/phpunit",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "alecrabbit/php-console-colour": "^0.10.0",
        "phpunit/php-timer": "^2.1.2 || ^3.0.0",
        "phpunit/phpunit": "^8.0.0 || ^9.0.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Testomat\\PHPUnit\\ResultPrinter\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/testomat/phpunit/issues",
        "source": "https://github.com/testomat/phpunit"
    }
}
